$TTL	604800
odns.io.	IN	SOA	vm2.odns.io. {{email}} (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
odns.io.	IN	NS	vm2

vm1	IN	A	{{ hostvars['alen-1']['ansible_default_ipv4']['address'] }}
vm2	IN	A {{ hostvars['alen-2']['ansible_default_ipv4']['address'] }}
